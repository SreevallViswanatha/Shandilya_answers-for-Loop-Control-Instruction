// WAP to find power of a number.
#include <stdio.h>
#include <stdlib.h>
#include<math.h>
main()
{
    int number, i=1, power=1, exponent;
    printf("Enter the base:");
    scanf("%d", &number);
    printf("Enter the exponent:");
    scanf("%d", &exponent);
    while(i<=exponent)
    {
        power=number*power;
        i++;
    }
    printf("%d^%d=%d", number, exponent, power);
}
