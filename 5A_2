// WAP to receive an integer and find its octal equivalent. (To obtain octal equivalent of an integer, divide it continuously by 8 till dividend doesnâ€™t become zero, then write the remainder obtained in reverse direction.)
#include <stdio.h>
#include <stdlib.h>
main()
{
    int number, octal=0, remainder, place=1;
    printf("Enter a number:");
    scanf("%d", &number);
    printf("The octal equivalent of %d is:", number);
    while(number>0)
    {
        remainder=number%8;
        number/=8;
        octal+=remainder*place;
        place*=10;
    }
    printf("%d", octal);
}
