// WAP to find LCM of two given numbers.
#include <stdio.h>
#include <stdlib.h>
main()
{
    int number1, number2, LCM=1, i;
    printf("Enter the first number:");
    scanf("%d", &number1);
    printf("Enter the second number:");
    scanf("%d", &number2);
    while(1)
    {
        if((LCM%number1==0)&&(LCM%number2==0))
        {
            i=LCM;
            break;
        }
        LCM++;
    }
    printf("The LCM of %d and %d is:%d.", number1, number2, i);
}
