// WAP to calculate factorial of a number.
#include <stdio.h>
#include <stdlib.h>
main()
{
    int number;
    unsigned long long factorial;
    printf("Enter a number:");
    scanf("%d", &number);
    factorial=number;
    while(number>1)
    {
        factorial=factorial*(number-1);
        number--;
    }
    printf("The factorial is:%llu.", factorial);
}
